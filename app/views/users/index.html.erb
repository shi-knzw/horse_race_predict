<div class="main users-index">
  <div class="container">
    <h1 class="users-heading">投稿者一覧</h1>
    <% @users.each do |user| %>
      <div class="users-index-item">
        <div class="user-left">
          <img src="<%= "/user_images/#{user.image_name}" %>">
        </div>
        <div class="user-right">
          <%= link_to(user.name, "/users/#{user.id}") %>
          <p style="color: blue;"><%= "#{user.name}さんのいいね！した数は#{user.user_likes_count}件です" %></p>
        </div>
      </div>
    <% end %>
  </div>
</div>